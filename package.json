{
  "name": "fm-hours",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "vercel": "vercel dev",
    "build": "nuxt build && nuxt generate",
    "start": "nuxt start",
    "test": "jest --passWithNoTests",
    "test:types": "tsc",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prettier": "prettier \"**/*.{js,ts,json,vue,scss}\" --ignore-path .gitignore --check",
    "format:eslint": "eslint --ext .js,.vue . --fix --ignore-path .eslintignore .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "prepare": "yarn husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "npx pretty-quick --staged --ignore-path .eslintignore,.gitignore"
    ],
    "*.{js, ts}": [
      "npm run test:types"
    ]
  },
  "dependencies": {
    "core-js": "3.23.5",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@babel/preset-env": "7.18.9",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@firebase/firestore-types": "^2.5.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/composition-api": "^0.33.0",
    "@nuxtjs/eslint-config": "10.0.0",
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/firebase": "8.2.2",
    "@nuxtjs/pwa": "3.3.5",
    "@types/jest": "28.1.6",
    "@types/jwt-decode": "3.1.0",
    "@vercel/node": "2.4.4",
    "@vue/test-utils": "2.0.2",
    "axios": "^0.27.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "28.1.3",
    "bootstrap": "4.6.1",
    "bootstrap-vue": "2.22.0",
    "date-fns": "2.28.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.2.1",
    "firebase": "9.6.5",
    "hotkeys-js": "3.9.4",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jwt-decode": "3.1.2",
    "lint-staged": "13.0.3",
    "node-sass": "7.0.1",
    "nuxt-i18n": "6.28.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "sass": "1.32.13",
    "sass-loader": "10.2.1",
    "standard-version": "9.5.0",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "vercel": "28.0.1",
    "vue-jest": "3.0.7",
    "vue-multiselect": "2.1.6"
  }
}
