{
  "name": "fm-hours",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --dotenv .env.dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "test:types": "tsc -p tsconfig.json --noEmit ",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lint": "npm run lint:eslint",
    "format:eslint": "eslint --ext .js,.vue . --fix --ignore-path .eslintignore .",
    "format:prettier": "npx pretty-quick --staged --ignore-path .eslintignore,.gitignore",
    "format": "npm run format:eslint && npm run format:prettier",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "prepare": "yarn husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run format:eslint",
      "npm run format:prettier"
    ],
    "*.{js, ts}": [
      "bash -c 'npm run test:types'"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@nuxtjs/eslint-config",
      "@nuxtjs/eslint-config-typescript",
      "eslint-config-prettier"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "7.15.6",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/composition-api": "0.29.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/firebase": "7.6.1",
    "@nuxtjs/proxy": "2.1.0",
    "@nuxtjs/pwa": "3.3.5",
    "@types/jest": "27.0.2",
    "@types/js-cookie": "2.2.7",
    "@types/jwt-decode": "3.1.0",
    "@vercel/node": "^1.12.1",
    "@vue/test-utils": "1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.2.1",
    "bootstrap": "~4.6.0",
    "bootstrap-vue": "2.21.2",
    "core-js": "3.18.0",
    "date-fns": "2.24.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "firebase": "8.0.1",
    "hotkeys-js": "3.8.7",
    "husky": "7.0.2",
    "jest": "27.2.1",
    "js-cookie": "2.2.1",
    "jwt-decode": "3.1.2",
    "lint-staged": "11.1.2",
    "nuxt": "2.15.8",
    "nuxt-i18n": "6.28.1",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "sass": "~1.32.13",
    "sass-loader": "10.1.1",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.5",
    "vue-jest": "3.0.7",
    "vue-multiselect": "2.1.6"
  }
}
